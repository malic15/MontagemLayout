            <div id="loaderContainerTop10"></div>
            <canvas id="topEventsChart" width="auto" height="120";></canvas>
        </div>

        <h1 class="fault-list-header">Lista de Anomalias Ativas</h1>
        <div id="faultList" class="fault-list"></div>

        <!-- Replay Panel -->
        <div id="replayPainel" class="replay-panel">
            <div style="display: flex; align-items: center; gap: 8px;">
                <label id="startTime" for="replayStartTime">Início:</label>
                <input type="datetime-local" id="replayStartTime" placeholder="Escolha o início">
                <button id="startReplayBtn">
                    <span class="btn-icon">&#11123;</span>
                    <span class="loading-spinner" style="display:none"></span>
                </button>


            </div>
            <div style="display: flex; align-items: center; gap: 8px;">
                <button id="replayPlayPauseBtn">▶️</button>
                <span id="replayCurrentTime">--:--:--</span>
                <input type="range" id="replaySlider" min="0" value="0" step="1">
                <select id="replaySpeed">
                    <option value="1000">1x</option>
                    <option value="500">2x</option>
                    <option value="200">5x</option>
                </select>
            </div>
            
            
        </div>

        <!-- Side Menu -->
        <aside id="sideMenu">
            <div class="side-menu-header">
                <span>Menu</span>
                <button id="sideMenuCloseBtn" title="Fechar">☰</button>
            </div>
            <nav class="side-menu-items">
                <a href="#" class="side-menu-item active"><i class="menu-icon">&#9638;</i> Layout</a>
                <a href="#" class="side-menu-item"><i class="menu-icon">&#8635;</i> Replay</a>
                <a href="#" class="side-menu-item"><i class="menu-icon">&#9881;</i> Configurações</a>
                <a href="#" class="side-menu-item"><i class="menu-icon">&#128065;</i> Esconder Acúmulo</a>
            </nav>
            <div class="side-menu-footer">
                <span>Status: <span class="status-indicator online"></span> Online</span>
            </div>
        </aside>

        <button id="sideMenuOpenBtn" title="Abrir Menu">☰</button>


    </div>
